    font-family: $nav-bar-servicios-font-family;
  }
  
  .img-boostrap-grilla {
    display: $img-boostrap-grilla-display;
    object-fit: $img-boostrap-grilla-object-fit;
    height: $img-boostrap-grilla-height;
    width: $img-boostrap-grilla-width;
  }
  
  .overlay {
    position: $overlay-position;
    top: $overlay-top;
    bottom: $overlay-bottom;
    left: $overlay-left;
    right: $overlay-right;
    opacity: $overlay-opacity;
    transition: $overlay-transition;
    background-color: $overlay-background-color;
  }
  
  .caption {
    font-size: $caption-font-size;
    position: $caption-position;
    top: $caption-top;
    left: $caption-left;
    transform: $caption-transform;
    text-align: $caption-text-align;
  }
  
  .thumbnail:hover {
    transform: $thumbnail-hover-transform;
