    height: $nav-ul-height;
    align-items: $nav-ul-align-items;
    justify-content: $nav-ul-justify-content;
  }
  
  .indexheader-1 {
    background-color: $indexheader-1-background-color;
    display: $indexheader-1-display;
    justify-content: $indexheader-1-justify-content;
    position: $indexheader-1-position;
