    cursor: $hover-contactoimagen1-cursor;
  }
  
  .indeximagen2:hover {
    padding: $hover-indeximagen2-padding;
    border: $hover-indeximagen2-border;
    cursor: $hover-indeximagen2-cursor;
  }
  
  a:hover {
    color: $hover-a-color;
    border: $hover-a-border;
    cursor: $hover-a-cursor;
  }
  
  .form-check-input:hover {
    border: $hover-form-check-input-border;
    cursor: $hover-form-check-input-cursor;
  }
