    font-family: $subtitulo-informacion-font-family;
    margin-top: $subtitulo-informacion-margin-top;
    margin-left: $subtitulo-informacion-margin-left;
  }
  
  .color {
    background-color: $color-background-color;
    text-decoration: $color-text-decoration;
    border: $color-border;
  }
  
  .form-label {
    margin-left: $form-label-margin-left;
  }
  
  .titulo-informacion {
    font-family: $titulo-informacion-font-family;
    color: $titulo-informacion-color;
    text-align: $titulo-informacion-text-align;
    font-size: $titulo-informacion-font-size;
    margin-top: $titulo-informacion-margin-top;
  }
  
  .texto-informacion {
    color: $texto-informacion-color;
    font-size: $texto-informacion-font-size;
    font-family: $texto-informacion-font-family;
    font-weight: $texto-informacion-font-weight;
    margin-top: $texto-informacion-margin-top;
    margin-left: $texto-informacion-margin-left;
  }
  
  // Estilos de hover
  
  button:hover {
    color: $hover-button-color;
    padding: $hover-button-padding;
    border: $hover-button-border;
    cursor: $hover-button-cursor;
  }
  
  .contactoimagen1:hover {
