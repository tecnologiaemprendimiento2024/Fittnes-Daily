// Variables
$color-primario: #91C8E4;
$color-secundario: #333;
$font-principal: Arial, sans-serif;

// Estilo global
body {
  font-family: $font-principal;
  background-color: #fff;
  color: $color-secundario;
}

a {
  color: $color-primario;
  text-decoration: none;
  &:hover {
    text-decoration: underline;
  }
}

// Navbar
.navbar {
  background-color: $color-primario;
  .container-fluid {
    .navbar-toggler-icon {
      background-color: $color-secundario;
    }
    .nav-link {
      color: $color-secundario;
      &.active {
        font-weight: bold;
      }
    }
  }
}

// Títulos
.titulo-informacion {
  color: $color-secundario;
}

.subtitulo-informacion {
  color: $color-primario;
}

// Contenedor de servicios
.con {
  padding: 20px;
  .thumbnail {
    border: 1px solid $color-primario;
    .img-container {
      position: relative;
      overflow: hidden;
      img {
        max-width: 100%;
        height: auto;
        transition: transform 0.3s ease;
      }
      .overplay {
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        background: rgba(0, 0, 0, 0.5);
        opacity: 0;
        transition: opacity 0.3s ease;
      }
      &:hover {
        img {
          transform: scale(1.1);
        }
        .overplay {
          opacity: 1;
        }
      }
    }
    .capition {
      text-align: center;
      padding: 20px;
      h2 {
        font-size: 24px;
        color: $color-primario;
      }
      h3 {
        font-size: 18px;
      }
      p {
        font-size: 16px;
      }
      .btn-primary {
        background-color: $color-primario;
        border-color: $color-primario;
        &:hover {
          background-color: darken($color-primario, 10%);
          border-color: darken($color-primario, 10%);
        }
      }
    }
  }
}

// Footer
footer {
  background-color: $color-primario;
  color: $color-secundario;
  text-align: center;
  padding: 10px 0;
}

// Formulario
.Container {
  margin: 20px;
  form {
    .form-label {
      font-weight: bold;
    }
    .form-control {
      margin-bottom: 10px;
    }
    .form-check-input {
      margin-right: 5px;
    }
    button {
      background-color: $color-primario;
      border-color: $color-primario;
      &:hover {
        background-color: darken($color-primario, 10%);
        border-color: darken($color-primario, 10%);
      }
    }
  }
}
