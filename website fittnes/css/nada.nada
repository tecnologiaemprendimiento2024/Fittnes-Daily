// Variables SASS para .overlay
$overlay-position: absolute;
$overlay-top: 0;
$overlay-bottom: 0;
$overlay-left: 0;
$overlay-right: 0;
$overlay-opacity: 0;
$overlay-transition: .5s ease;
$overlay-background-color: rgba(255, 255, 255, 0.9);

// Variables SASS para .caption
$caption-font-size: 2rem;
$caption-position: absolute;
$caption-top: 50%;
$caption-left: 50%;
$caption-transform: translate(-50%, -50%);
$caption-text-align: center;

// Variables SASS para :hover en .thumbnail
$thumbnail-hover-transform: translateY(-10px);
$thumbnail-hover-background-color: rgba(255, 255, 255, 0.9);

// Variables SASS para :hover en .thumbnail-1
$thumbnail-1-hover-transform: translateY(-10px);
$thumbnail-1-hover-background-color: rgba(255, 255, 255, 0.9);

// Variables SASS para .btn
$btn-font-family: 'Montserrat', sans-serif;


